</header>

    <section class="homemainsect">
      <div class="container">
        <div class="row holdhomeshift">
          <div class="col-lg-5">
          <div class="holdintrotextdone">
            <h1 class="welcomet" style="color: #ffffff;">Welcome Back!</h1><br>
            <p style="" class="holdintrohometext">
             Welcome back to the kuul-side. Sign in to find out what's new, fun and most importantly cool! Your kids will be shocked you know what's up - don't tell them your secret!
            </p>
            <br>
            
            <p style="color: white; font-size: 16px; text-shadow: 1px 1px 2px rgba(0, 0, 0, 0.3);">
              Not a member? Click <a style="color: white;" href="/users/sign_up"><u>here</u></a> to join
            </p>
          </div>
        </div>
        <div class="col-lg-2">

        </div>
        <div class="col-lg-5">
          <div class="holdforsignup">

            <p>
              <a href="/users/auth/google_oauth2" class="btn btn-primary btn-lg googleloginbtn" style="width: 100%;">Continue with Google</a><br>
              <a href="/users/auth/facebook" class="btn btn-primary btn-lg facebookloginbtn" style="width: 100%; margin-top: 20px;">Continue with Facebook</a>
            </p>

            <p style="text-align: center; color: #4a4a4a; font-size: 20px;">OR</p>
            <p style="color: #f17100; text-align: center; font-size: 14px; font-weight: 600;">Sign up with your Email and Password</p>
            
            <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

            <div class="form-group">
              <label style="color: #f17100; font-size: 14px;">Email</label>
              <%= f.email_field :email, autofocus: true, autocomplete: "email", :class => "form-control", :placeholder => "Your Email" %>
            </div>

            <div class="form-group">
              <label style="color: #f17100; font-size: 14px;">Password</label>
              <%= f.password_field :password, autocomplete: "new-password", :class => "form-control", :placeholder => "Your Password" %>
            </div>

             <% if devise_mapping.rememberable? %>
              <div class="form-group">
                <%= f.check_box :remember_me %>
                <%= f.label :remember_me %>
              </div>
            <% end %>

            <div class="form-group" style="margin-top: 40px;">
              <%= f.submit "Sign In", :class => "btn-primary btn-lg kuulishsignupbtn" %>
            </div>
          <% end %>

          

          </div>
        </div>
        </div>
      </div>
    </section>

    <script type="text/javascript">
$(window).scroll(function () {
        if ($(window).scrollTop() > 50) {
            $(".navbackground").addClass('meffect');
            
        } else {
            $(".navbackground").removeClass('meffect');
        }
    });

    </script>