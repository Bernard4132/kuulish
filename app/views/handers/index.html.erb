<p id="notice"><%= notice %></p>

<h1>Handers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @handers.each do |hander| %>
      <tr>
        <td><%= hander.name %></td>
        <td><%= link_to 'Show', hander %></td>
        <td><%= link_to 'Edit', edit_hander_path(hander) %></td>
        <td><%= link_to 'Destroy', hander, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Hander', new_hander_path %>
